    "def fact(x):\n",
    "    if x==0 or x==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return x*fact(x-1)\n",
    "n=fact(int(input(\"Enter a number :\")))\n",
    "print(n)\n"
